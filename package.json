{
	"name": "ldm-taro-frc",
	"version": "0.7.4",
	"description": "拉旦木小程序-功能组件(基于taro)",
	"templateInfo": {
		"name": "redux",
		"typescript": false,
		"css": "less"
	},
	"keywords": [
		"ldm",
		"taro",
		"mp",
		"frc"
	],
	"main": "lib/index.js",
	"files": [
		"lib/**"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
